<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.catering">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".book" />
        <activity android:name=".membership" />
        <activity android:name=".resetPass" />
        <activity android:name=".userFoodDetails" />
        <activity android:name=".foodList" />
        <activity android:name=".addService" />
        <activity android:name=".changeOrderStatus" />
        <activity
            android:name=".serviceDetails"
            android:label="Service Details"/>

        <activity
            android:name=".mainInterface"
            android:label="@string/title_activity_main_interface"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
